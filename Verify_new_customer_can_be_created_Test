    driver.findElement(By.name("pinno")).sendKeys("000000");
    driver.findElement(By.name("telephoneno")).sendKeys("6666123456");
    driver.findElement(By.name("emailid")).sendKeys("tan.test@yopmail.com");
    driver.findElement(By.name("password")).sendKeys("1234567890");
    driver.findElement(By.name("sub")).click();
