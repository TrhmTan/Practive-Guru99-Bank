  @Test
  public void verifynewcustomercanbecreated() {
    driver.get("http://demo.guru99.com/v4/manager/addcustomerpage.php");
    driver.manage().window().setSize(new Dimension(1346, 708));
    driver.findElement(By.name("name")).click();
